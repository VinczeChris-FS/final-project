{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app", "airbnb"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": "off",
    "no-unused-vars": "warn",
    "no-console": "off",

    "react/jsx-no-bind": [
      1,
      {
        "allowFunctions": true
      }
    ]
  }
}
